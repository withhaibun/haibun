{
  "name": "@haibun/core",
  "type": "module",
  "version": "1.15.1",
  "description": "",
  "author": "",
  "xexports": {
    "./lib/*": "./build/lib/*.js",
    "./util/*": "./build/util/*.js",
    "./test/*": "./build/test/*.js"
  },
  "xtypesVersions": {
    "*": {
      "build/*": [
        "build/*"
      ]
    }
  },
  "files": [
    "build/**"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint -c .eslintrc.json --ext .ts,.js src",
    "test": "jest",
    "coverage": "jest --coverage",
    "test-watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "build": "tsc -b .",
    "tsc-watch": "tsc --watch",
    "tsc": "tsc",
    "prepublishOnly": "tsc -b .",
    "preversion": "npm run lint",
    "xversion": "npm run format && git add -A src"
  }
}
